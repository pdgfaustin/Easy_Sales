	<queryString>
		<![CDATA[SELECT
     les_ventes.`idClie` AS les_ventes_idClie,
     les_ventes.`idArticles` AS les_ventes_idArticles,
     les_ventes.`idSite` AS les_ventes_idSite,
     les_ventes.`idTVentes` AS les_ventes_idTVentes,
     les_ventes.`numFact` AS les_ventes_numFact,
