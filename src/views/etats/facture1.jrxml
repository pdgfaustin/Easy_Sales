     les_ventes.`dateVente` AS les_ventes_dateVente,
     les_ventes.`users` AS les_ventes_users,
     les_ventes.`desiArticle` AS les_ventes_desiArticle,
     les_ventes.`libSite` AS les_ventes_libSite,
     les_ventes.`adresse` AS les_ventes_adresse
FROM
     `les_ventes` les_ventes
WHERE
     numFact = $P{hFacture}
 AND idSite = $P{site}
GROUP BY
     idArticles,qteVente,prixVente]]>
