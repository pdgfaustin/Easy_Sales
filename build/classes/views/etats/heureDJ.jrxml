	<queryString>
		<![CDATA[SELECT
     SUM(ventes.`qteVente`) AS ventes_qteVente,
     SUM(ventes.`prixVente`*ventes.`qteVente`) AS ventes_prixVente,
     ventes.`idArticles` AS ventes_idArticles,
     ventes.`idSite` AS ventes_idSite
FROM
     `ventes` ventes
WHERE
     ventes.`idSite` = $P{site} AND ventes.`idTVentes` = "HEURE DE JOIE" AND ventes.`idventes` BETWEEN $P{numeroD} AND $P{numeroF}
GROUP BY
     ventes.`idArticles`,
     ventes.`idSite`]]>
	</queryString>
